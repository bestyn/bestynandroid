<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:shape="oval">
            <solid
                android:color="@color/story_switch_thumb_color" />
            <size
                android:width="@dimen/story_switch_thumb_size"
                android:height="@dimen/story_switch_thumb_size" />
            <stroke
                android:width="@dimen/story_switch_thumb_spacing"
                android:color="#0000ffff" />
        </shape>
    </item>
</selector>